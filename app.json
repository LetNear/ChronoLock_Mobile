{
  "expo": {
    "name": "ChronoLock",
    "slug": "ChronoLock_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chronolock-logo2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markvincentcleofe06.ChronoLock_Mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3f1f1b8c-d58b-4f55-b37b-a6913c18dbbc"
      }
    }
  }
}
